{
    "name": "mrzainulabideen/aesencrypt",
    "type": "library",
    "description": "Laravel MySql AES Encrypt/Decrypt",
    "keywords": ["laravel","mysql", "aes", "encrypt"],
    "homepage": "https://github.com/mrzainulabideen/mysql-aes-encrypt",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Zain Ul Abideen",
            "email": "mrzainulabideen2037@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/config": "^7.0",
        "illuminate/database": "^7.0",
        "illuminate/support": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "mrzainulabideen\\AESEncrypt\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "redsd\\AESEncrypt\\AesEncryptServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
